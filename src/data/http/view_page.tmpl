<html>
<head>
	<title>DViz Web Viewer</title>
	<link rel="shortcut icon" type="image/x-icon" href="/:/data/http/favicon.ico">
	<link rel="stylesheet" type="text/css" href='/:/data/http/styles.css' />
	<script src="/:/data/http/view-scripts.js"></script>
	<meta name="viewport" content="user-scalable=no, width=320" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	
</head>
<body class='viewer'>
	<h1 id='doc_name'>%%doc_name%%</h1>
	
	<table border='0'>
		<tr>
			<td align='center'>
				<div id='group_name'>%%group_name%%</div>
				<img src='/image' id='view_image'>
				<div id='slide_name'>%%slide_name%%</div>
			</td>
		</tr>
	</table>
	
	<script>
		window.CurrentSlideID = "%%slide_id%%";
		window.CurrentSlideName = "%%slide_name%%";
		startPolling();
	</script>
	
	<div id='dviz-link'>
		<a href="http://code.google.com/p/dviz"><img src="/:/data/icon-d.png" align="left" width='16' height='16' border='0'> 
		Powered by <a href="http://code.google.com/p/dviz"><b>DViz</b></a>, &copy; 2010 Josiah Bryan
	</div>
</body>
</html>
